   29                  | KEY '(' column_list ')'

   30 column_atts: %empty
   31            | column_atts NOT NULLX
   32            | column_atts NULLX
   33            | column_atts STRING
   34            | column_atts INTNUM
   35            | column_atts FLOAT
   36            | column_atts PRIMARY KEY

   37 opt_length: %empty
