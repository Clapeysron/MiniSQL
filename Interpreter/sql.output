   57 table_reference: NAME

   58 stmt: drop_stmt

   59 drop_stmt: DROP TABLE NAME
   60          | DROP KEY NAME ON NAME

   61 stmt: show_stmt

   62 show_stmt: SHOW TABLES
   63          | SHOW STATUS

   64 stmt: exit_stmt

   65 exit_stmt: EXIT

   66 expr: NAME
