   17 stmt: create_table_stmt

   18 create_table_stmt: CREATE TABLE NAME '(' create_col_list ')'

   19 stmt: create_index_stmt

   20 create_index_stmt: CREATE KEY NAME ON NAME '(' index_expr_list ')'

   21 index_expr_list: expr
   22                | index_expr_list ',' expr
   23                | '*'

   24 create_col_list: create_definition
