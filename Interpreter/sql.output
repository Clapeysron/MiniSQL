   42          | VARCHAR '(' INTNUM ')'

   43 column_list: NAME
   44            | column_list ',' NAME

   45 stmt: select_stmt

   46 select_opts: %empty

   47 select_stmt: SELECT select_opts select_expr_list
   48            | SELECT select_opts select_expr_list FROM table_references opt_where

   49 opt_where: %empty
   50          | WHERE expr

   51 select_expr_list: select_expr
   52                 | select_expr_list ',' select_expr
   53                 | '*'

   54 select_expr: expr

   55 table_references: table_reference
